{
    "version": 1,
    "formatters": {
        "brief": {
            "format": "%(levelname)-8s: %(name)-15s %(message)s"
        },
        "precise": {
            "format": "%(asctime)s %(name)-12s %(levelname)-8s %(message)s"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "formatter": "brief",
            "level": "DEBUG"
        }
    },
    "root": {
        "handlers": [
            "console"
        ],
        "level": "DEBUG"
    }
}